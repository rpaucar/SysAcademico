div.datepicker { position: relative; font-family: Arial, Helvetica, sans-serif; font-size: 14px; width: 196px; height: 147px; position: absolute; cursor: default; top: 0; left: 0; display: none; background: #68c8f6; -moz-border-radius: 6px; border-radius: 6px; -moz-box-shadow: 0 0 3px #959595; -webkit-box-shadow: 0 0 3px #959595; box-shadow: 0 0 3px #959595; }

.datepickerContainer { background: #f6f6f6; position: absolute; top: 3px; left: 3px; border: 1px solid #fff; padding: 7px; -moz-border-radius: 4px; border-radius: 4px; }
